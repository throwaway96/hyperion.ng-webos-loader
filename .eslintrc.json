{
    "root": true,
    "extends": [
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "no-console": "off",
        "no-restricted-syntax": "off"
    },
    "ignorePatterns": [
        "lib/**",
        "node_modules/**",
        "dist/**",
        "hyperion.ng/**",
        "service/**",
        "tools/**"
    ]
}
